generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model CV {
  id        String   @id @default(uuid())
  filename  String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Experience {
  id String   @id @default(uuid())
  company String
  position String
  startDate DateTime
  endDate DateTime?
  description String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}